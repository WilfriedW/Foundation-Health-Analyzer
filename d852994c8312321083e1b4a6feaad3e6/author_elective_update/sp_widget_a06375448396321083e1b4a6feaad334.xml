        <template><![CDATA[<div class="container">
  <h2>]]>üè•<![CDATA[ Foundation Health Analyzer</h2>
  
  <div class="card">
    <h3>]]>üìä<![CDATA[ Table Analysis</h3>
    
    <div class="form-group">
      <label for="tableSelect">Select Table:</label>
      <select id="tableSelect" class="form-control">
        <option value="">-- Select a table --</option>
      </select>
    </div>
    
    <button id="analyzeBtn" class="btn btn-primary" onclick="analyzeTable()">
      <i class="fa fa-play"></i> Analyze Table
    </button>
    
    <div id="loading" style="display:none;">
      <i class="fa fa-spinner fa-spin"></i> Analyzing...
    </div>
  </div>
  
  <div id="results" style="display:none;"></div>
</div>

<style>
.container { max-width: 800px; margin: 20px auto; }
.card { background: #fff; border: 1px solid #ddd; padding: 20px; margin: 20px 0; }
.btn { padding: 10px 20px; background: #0378b8; color: white; border: none; cursor: pointer; }
.btn:hover { background: #035b8a; }
.form-group { margin: 15px 0; }
.form-control { width: 100%; padding: 8px; }
</style>

<script>
// Load available tables
function loadTables() {
  c.server.get({
    action: 'getTables'
  }).then(function(response) {
    var select = document.getElementById('tableSelect');
    select.innerHTML = '<option value="">-- Select a table --</option>';
    
    if (response.data && response.data.tables) {
      response.data.tables.forEach(function(table) {
        var option = document.createElement('option');
        option.value = table.name;
        option.textContent = table.label + ' (' + table.name + ')';
        select.appendChild(option);
      });
    }
  });
}

// Analyze selected table
function analyzeTable() {
  var tableSelect = document.getElementById('tableSelect');
  var tableName = tableSelect.value;
  
  if (!tableName) {
    alert('Please select a table');
    return;
  }
  
  document.getElementById('analyzeBtn').disabled = true;
  document.getElementById('loading').style.display = 'block';
  
  c.server.get({
    action: 'analyze',
    table_name: tableName
  }).then(function(response) {
    document.getElementById('analyzeBtn').disabled = false;
    document.getElementById('loading').style.display = 'none';
    
    if (response.data.success) {
      // Redirect to results widget
      window.location = '?id=fha_results&analysis_id=' + response.data.analysis_id;
    } else {
      alert('Error: ' + response.data.error);
    }
  });
}

// Initialize
loadTables();
</script>
]]></template>
