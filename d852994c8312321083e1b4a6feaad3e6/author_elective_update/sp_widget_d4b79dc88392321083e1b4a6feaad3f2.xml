<?xml version="1.0" encoding="UTF-8"?><record_update table="sp_widget">
    <sp_widget action="DELETE">
        <category>custom</category>
        <client_script><![CDATA[api.controller=function($scope, $location) {
  /* widget controller */
  var c = this;

  
  // Initialisation
  c.data = {
    currentPage: 'dashboard',
    userName: '',
    instanceName: '',
    hasAnalysis: false,
    loading: false,
    lastAnalysis: null
  };
  
  // Détecter la page courante
  c.$onInit = function() {
    c.detectCurrentPage();
    
    // Écouter les changements de page
    $scope.$on('$locationChangeSuccess', function() {
      c.detectCurrentPage();
    });
  };
  
  // Détecter la page actuelle
  c.detectCurrentPage = function() {
    var search = $location.search();
    c.data.currentPage = search.id || 'dashboard';
    
    // Vérifier si on a une analyse en cours
    c.data.hasAnalysis = !!search.analysis_id;
    
    // Mettre à jour le nom d'utilisateur et l'instance
    c.data.userName = $scope.user ? $scope.user.first_name + ' ' + $scope.user.last_name : '';
    
    if (!$scope.user || $scope.user.first_name === '') {
      c.data.userName = $scope.user ? $scope.user.user_name || '' : '';
    }
  };

};]]></client_script>
