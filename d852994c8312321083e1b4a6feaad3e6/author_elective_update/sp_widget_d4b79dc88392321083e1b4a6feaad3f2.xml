        <script><![CDATA[(function() {
  // Récupérer le nom de l'instance
  data.instanceName = gs.getProperty('instance_name') || 'ServiceNow';
  
  // Récupérer la dernière analyse
  var gr = new GlideRecord('u_fha_results');
  gr.orderByDesc('started_at');
  gr.setLimit(1);
  gr.query();
  
  if (gr.next()) {
    var date = new GlideDateTime(gr.getValue('started_at'));
    data.lastAnalysis = date.getDisplayValue();
  }
})();
]]></script>
