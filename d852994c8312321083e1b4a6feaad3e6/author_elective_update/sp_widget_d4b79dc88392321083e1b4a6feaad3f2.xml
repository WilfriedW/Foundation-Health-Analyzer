        <li ng-class="{active: c.data.currentPage === 'analysis_details'}">
          <a href="?id=analysis_details" ng-if="c.data.hasAnalysis">
            <i class="fa fa-chart-bar"></i> Résultats
          </a>
        </li>
        <li>
          <a href="?id=history">
            <i class="fa fa-history"></i> Historique
          </a>
        </li>
        <li>
          <a href="?id=configuration">
            <i class="fa fa-cog"></i> Configuration
          </a>
        </li>
        <li>
          <a href="?id=documentation">
            <i class="fa fa-book"></i> Documentation
          </a>
        </li>
      </ul>
      
      <!-- Indicateur de chargement -->
      <ul class="nav navbar-nav navbar-right">
        <li ng-if="c.data.loading">
          <a href style="color: #0378BE;">
            <i class="fa fa-spinner fa-spin"></i>
